		a1.add("C++");
		a1.add("C");
		a1.add("PYTHON");
		a1.add(".NET");
		
		System.out.println(a1);
		//get elements by index
		System.out.println("Element at index 1: "+a1.get(1));
		System.out.println("Does list conatins JAVA?"+a1.contains("JAVA"));
		
		//add elements at a specific index
		a
	}

}
